PORT=3000
ETH_NODE_WS=wss://<URL>.com/<VERSION>/<API_KEY> # Alchemy
GRACE_PERIOD_MS=30000 # Startup grace period before health check fails (default: 30000ms)
GAS_PRICE_STALENESS_MS=20000 # How long before cached gas data is considered stale (default: 20000ms)

# Rate Limiting
THROTTLE_TTL_MS=60000 # Time window in milliseconds (default: 60000ms = 1 minute)
THROTTLE_LIMIT=1100 # Max requests per IP per time window (default: 1100)
